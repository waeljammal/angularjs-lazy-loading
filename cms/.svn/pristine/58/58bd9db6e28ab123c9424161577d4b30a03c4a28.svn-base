<!-- Header -->
<div data-ng-include=" 'admin/core/tpl/blocks/header.html' " class="app-header navbar">
</div>

<!-- Left Menu -->
<div data-ng-include=" 'admin/core/tpl/blocks/aside.html' " class="app-aside hidden-xs {{app.settings.asideColor}}">
</div>

<!-- Content -->
<div class="app-content">
    <div class="app-content-body app-content-full fade-in-up" ui-view></div>
</div>

<!-- Footer -->
<div class="app-footer wrapper b-t bg-light">
    <span class="pull-right">{{app.version}}
        <a href ui-scroll="app" class="m-l-sm text-muted">
            <i class="fa fa-long-arrow-up"></i>
        </a>
    </span>
    &copy; 2014 Copyright.
</div>