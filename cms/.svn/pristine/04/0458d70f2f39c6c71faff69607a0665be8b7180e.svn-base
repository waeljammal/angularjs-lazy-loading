<!-- Header -->
<div data-ng-include=" 'admin/core/tpl/blocks/header.html' " class="app-header navbar">
</div>


<!-- Left Menu -->
<div data-ng-include=" 'admin/core/tpl/blocks/aside.html' " class="app-aside hidden-xs {{app.settings.asideColor}}">
</div>

